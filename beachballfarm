if game.PlaceId == 142823291 then
    local Players = game:GetService('Players')
    local CoreGUI = game:GetService('CoreGui')
    local UserInputService = game:GetService("UserInputService")
    local LocalPlayer = Players.LocalPlayer

    getgenv().coinFarm = false
    local maxDistance = 200
    local stepSize = 11
    local waitPerStep = 0.03
    local collectedCoins = 0

    -- GUI
    local ScreenGui = Instance.new("ScreenGui")
    ScreenGui.Name = "MM2Farm"
    ScreenGui.ResetOnSpawn = false
    ScreenGui.IgnoreGuiInset = true
    ScreenGui.Parent = CoreGUI

    local Frame = Instance.new("Frame")
    Frame.Size = UDim2.new(0, 170, 0, 110)
    Frame.Position = UDim2.new(0, 100, 0, 100)
    Frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    Frame.BorderSizePixel = 0
    Frame.Parent = ScreenGui
    Instance.new("UICorner", Frame).CornerRadius = UDim.new(0, 10)

    local Title = Instance.new("TextLabel")
    Title.Size = UDim2.new(1, 0, 0, 25)
    Title.Position = UDim2.new(0, 0, 0, 0)
    Title.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    Title.TextColor3 = Color3.new(1, 1, 1)
    Title.Font = Enum.Font.GothamBold
    Title.TextSize = 14
    Title.Text = "Vefarm MM2 | Summer"
    Title.Parent = Frame
    Instance.new("UICorner", Title).CornerRadius = UDim.new(0, 8)

    local Button = Instance.new("TextButton")
    Button.Size = UDim2.new(1, -10, 0, 35)
    Button.Position = UDim2.new(0, 5, 0, 30)
    Button.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    Button.TextColor3 = Color3.new(1, 1, 1)
    Button.Font = Enum.Font.GothamBold
    Button.TextSize = 14
    Button.Text = "AutoFarm: OFF"
    Button.Parent = Frame
    Instance.new("UICorner", Button).CornerRadius = UDim.new(0, 8)

    local CoinLabel = Instance.new("TextLabel")
    CoinLabel.Size = UDim2.new(1, -10, 0, 30)
    CoinLabel.Position = UDim2.new(0, 5, 0, 70)
    CoinLabel.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    CoinLabel.TextColor3 = Color3.new(1, 1, 1)
    CoinLabel.Font = Enum.Font.Gotham
    CoinLabel.TextSize = 13
    CoinLabel.Text = "Coins Collected: 0"
    CoinLabel.Parent = Frame
    Instance.new("UICorner", CoinLabel).CornerRadius = UDim.new(0, 6)

    -- Drag
    local dragging, dragInput, dragStart, startPos
    Frame.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            dragStart = input.Position
            startPos = Frame.Position
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false
                end
            end)
        end
    end)
    Frame.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement then
            dragInput = input
        end
    end)
    UserInputService.InputChanged:Connect(function(input)
        if input == dragInput and dragging then
            local delta = input.Position - dragStart
            Frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
        end
    end)

    -- Teleport
    local function stepBypassTo(pos)
        local char = LocalPlayer.Character
        if not char or not char:FindFirstChild("HumanoidRootPart") then return end
        local hrp = char.HumanoidRootPart
        local distance = (hrp.Position - pos).Magnitude
        local direction = (pos - hrp.Position).Unit
        local steps = math.ceil(distance / stepSize)
        for _ = 1, steps do
            local stepPos = hrp.Position + direction * math.min(stepSize, (pos - hrp.Position).Magnitude)
            hrp.CFrame = CFrame.new(stepPos)
            task.wait(waitPerStep)
        end
    end

    -- Coins
    local function getNearestCoins(maxCount)
        local char = LocalPlayer.Character
        if not char or not char:FindFirstChild("HumanoidRootPart") then return {} end
        local hrp = char.HumanoidRootPart
        local found = {}
        for _, obj in pairs(workspace:GetDescendants()) do
            if obj:IsA("Part") and obj.Name == "Coin_Server" then
                local dist = (hrp.Position - obj.Position).Magnitude
                if dist < maxDistance then
                    table.insert(found, {dist = dist, coin = obj})
                end
            end
        end
        table.sort(found, function(a, b) return a.dist < b.dist end)
        local result = {}
        for i = 1, math.min(maxCount, #found) do
            table.insert(result, found[i].coin)
        end
        return result
    end

    -- AutoFarm
    local function coinFarm()
        while getgenv().coinFarm do
            local coins = getNearestCoins(3)
            for _, coin in pairs(coins) do
                if coin and coin:IsDescendantOf(workspace) then
                    stepBypassTo(coin.Position + Vector3.new(0, 4, 0))
                    collectedCoins += 1
                    CoinLabel.Text = "Coins Collected: " .. collectedCoins
                    task.wait(0.05)
                end
            end
            task.wait(0.15)
        end
    end

    -- Button
    local toggled = false
    Button.MouseButton1Click:Connect(function()
        toggled = not toggled
        getgenv().coinFarm = toggled
        if toggled then
            collectedCoins = 0
            CoinLabel.Text = "Coins Collected: 0"
            Button.Text = "AutoFarm: ON"
            Button.BackgroundColor3 = Color3.fromRGB(40, 100, 40)
            task.spawn(coinFarm)
        else
            Button.Text = "AutoFarm: OFF"
            Button.BackgroundColor3 = Color3.fromRGB(100, 40, 40)
        end
    end)
end
